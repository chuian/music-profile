generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
}

model MusicProfile {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  name           String
  genre          String
  vibes          String
  crowd          String
  experience     String
  suggestedFood  String?
  suggestedDrink String?
}
